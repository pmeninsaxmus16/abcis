{% extends "BookingBundle::layoutMatto.html.twig" %}
{% block title%} Notification Show {%endblock%}
{% block scripthead %}
 <script src="{{ asset('Resources/js/bootstrap-dropdown.js') }}"></script>
{% endblock %}

{% block bodycontenido%}
  {#  <h1>BkNotification</h1>  #}

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Resource id</th>
                <td>{{ entity.resourceId }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ entity.quantity }}</td>
            </tr>
            <tr>
                <th>Booking id</th>
                <td>{{ entity.bookingId }}</td>
            </tr>
            <tr>
                <th>Event id</th>
                <td>{{ entity.eventId }}</td>
            </tr>
            <tr>
                <th>Performer</th>
                <td>{{ entity.performer }}</td>
            </tr>
            <tr>
                <th>Activity type</th>
                <td>{{ entity.activityType }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.ip }}</td>
            </tr>
            <tr>
                <th>Updated date</th>
                <td>{{ entity.updatedDate|date('d-m-Y') }}</td>
            </tr>
        </tbody>
    </table>
<p> <button class="btn" data-dismiss="modal" aria-hidden="true"><i class="icon-list"></i> Back to the list</button> </p>
