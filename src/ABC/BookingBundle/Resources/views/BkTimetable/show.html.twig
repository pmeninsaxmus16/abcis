{% extends "BookingBundle::layoutMatto.html.twig" %}
{% block title %} Timetable Show {% endblock %}
{% block scripthead %}
 <script src="{{ asset('Resources/js/bootstrap-dropdown.js') }}"></script>
{% endblock %}


{% block bodycontenido %}
  {# <h1>BkTimetable</h1> #}

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Nickname</th>
                <td>{{ entity.nickname }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Header id</th>
                <td>{{ entity.headerId }}</td>
            </tr>
            <tr>
                <th>Start time</th>
                <td>{{ entity.startTime|date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>End time</th>
                <td>{{ entity.endTime|date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>Created date</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>
 <p>
    <button class="btn" data-dismiss="modal" aria-hidden="true"><i class="icon-list"></i> Back to the list</button>
 </p>
