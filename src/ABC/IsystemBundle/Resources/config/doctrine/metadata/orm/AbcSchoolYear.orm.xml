<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AbcSchoolYear" table="abc_school_year">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="10" nullable="false"/>
    <field name="code" type="string" column="code" length="10" nullable="false"/>
    <field name="startDate" type="date" column="start_date" nullable="false"/>
    <field name="endDate" type="date" column="end_date" nullable="false"/>
    <field name="active" type="string" column="active" nullable="false"/>
    <field name="nextYear" type="string" column="next_year" nullable="false"/>
  </entity>
</doctrine-mapping>
