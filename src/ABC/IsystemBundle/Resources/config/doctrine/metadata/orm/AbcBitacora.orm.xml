<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AbcBitacora" table="abc_bitacora">
    <id name="idCard" type="string" column="id_card" length="10">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="operacion" type="string" column="operacion" length="10" nullable="true"/>
    <field name="usuario" type="string" column="usuario" length="10" nullable="true"/>
    <field name="host" type="string" column="host" length="15" nullable="true"/>
    <field name="modificado" type="datetime" column="modificado" nullable="true"/>
    <field name="tabla" type="string" column="tabla" length="80" nullable="true"/>
  </entity>
</doctrine-mapping>
