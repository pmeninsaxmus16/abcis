<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AbcSadulation" table="abc_sadulation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="abbreviationEn" type="string" column="abbreviation_en" length="10" nullable="true"/>
    <field name="abbreviationEs" type="string" column="abbreviation_es" length="10" nullable="false"/>
    <field name="saludationEn" type="string" column="saludation_en" length="45" nullable="true"/>
    <field name="saludationEs" type="string" column="saludation_es" length="45" nullable="false"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="false"/>
  </entity>
</doctrine-mapping>
