<!DOCTYPE html>
<html lang="en">
<head>
   {% block meta %}
   <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   {% endblock %}
   
   <title>{% block title %}UP REPORTS{% endblock %}</title>

   {% block scriptheadprimary %}
   <link href="{{ asset('Resources/css/bootstrap.css') }}" rel="stylesheet" media="all">
   <link href="{{ asset('Resources/css/bootstrap-responsive.css') }}" rel="stylesheet" media="all">
   <script src="{{ asset('Resources/js/jquery.js') }}"></script>
   <script src="{{ asset('Resources/js/bootstrap.js') }}"></script>  
   <script src="{{ asset('Resources/js/bootstrap-alert.js') }}"></script>
   <script src="{{ asset('Resources/js/bootstrap-tooltip.js') }}"></script>
   <script src="{{ asset('Resources/js/bootstrap-popover.js') }}"></script>
   <script src="{{ asset('Resources/js/bootstrap-dropdown.js') }}"></script>
   {% endblock %}
   {% block scripthead %}{% endblock %}
   <style type="text/css">{% block stylesheets %}{% endblock %}</style> 
</head>

<body>
{# ###### BLOCK CONTIENTE TODO EL BODY ##### #}
{% block body %}
<div class="row-fluid">
  <div class="span12">
   {# ###### BLOCK MUESTRA EL MENU ##### #}
   {% block menu %}	
   {% endblock %}
   {# ###### BLOCK MUESTRA EL MENU ##### #}
  </div>
</div>
<div class="container-fluid">
{# ###### BLOCK MUESTRA EL BODY ##### #}
	<div class="row-fluid">
	{% block bodycontenido %}
		{# ###### BLOCK MUESTRA EL CONTENIDO A LA DERECHA ##### #}
		<div class="span3">{% block sidebar %}{% endblock %}</div>
    	        {# ###### BLOCK MUESTRA EL CONTENIDO A LA DERECHA ##### #}

		{# ###### BLOCK MUESTRA EL CONTENIDO A LA DERECHA ##### #}
		<div class="span9">{% block contenido %}{% endblock %}</div>
		{# ###### BLOCK MUESTRA EL CONTENIDO A LA DERECHA ##### #} 
	{% endblock %}
	</div>
	{# ###### BLOCK MUESTRA EL BODY ##### #}
</div>
{% endblock %}
{# ###### BLOCK CONTIENTE TODO EL BODY ##### #}

{# ###### BLOCK PARA JAVASCRIPTS ##### #}
{% block javascripts %}

{% endblock %}   
{% block javascriptsOthers %}

{% endblock %}   
{# ###### BLOCK PARA JAVASCRIPTS ##### #}   
</body>
</html>
